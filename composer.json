{
    "name": "fkeloks/simple-container",
    "description": "A simple psr-11 container for PHP >= 7.0",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "fkeloks",
            "email": "fkeloks@gmail.com"
        }
